
@{
    ViewBag.Title = "Dashboard";
}

<div class="container-fluid">
    <div class="row">
        <div class="col-sm-3 col-md-2 sidebar">
            <ul class="nav nav-sidebar">
                <li><a href="#">Home <span class="sr-only">(current)</span></a></li>
                <li class="active"><a href="/Home/DashBoard">DashBoard</a></li>
                <li><a href="/Home/ViewRecords">Volunteers</a></li>
                <li><a href="#">Events</a></li>
                <li><a href="">Content</a></li>
                <li><a href="">Users</a></li>
                <li><a href="">Evaluations</a></li>
                <li><a href="">Settings</a></li>
                <li><a href="">My Profile</a></li>
                <li><a href="">Logout</a></li>
            </ul>
        </div>
        <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
            <h1 class="page-header">Users registered by hour/ User Breakdown by Age</h1>

            <div class="table-responsive">
                <div class="col-lg-6 col-sm-12"  id="line-chart" ng-controller="LineCtrl1">
                    <div class="panel panel-default">
                        <div class="panel-heading">Users registered by hour</div>
                        <div class="panel-body">
                            <canvas id="line" class="chart chart-line" data="data" labels="labels" legend="true"
                                    click="onClick" hover="onHover" series="series"></canvas>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6 col-sm-12" id="line-chart1" ng-controller="LineCtrl2">
                    <div class="panel panel-default">
                        <div class="panel-heading">Users break down by age</div>
                        <div class="panel-body">
                          <canvas id="line1" class="chart chart-line" data="data" labels="labels" legend="true"
                                  click="onClick1" hover="onHover" series="series"></canvas>
           </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section scripts{
<link rel="stylesheet" href="Content/angular-chart.css">
<script src="/Scripts/angular.js"></script>
<script src="/Scripts/chart.min.js"></script>   
<script src="/Scripts/angular-chart.js"></script>
<script src="/Scripts/DashBController.js"></script>
}

